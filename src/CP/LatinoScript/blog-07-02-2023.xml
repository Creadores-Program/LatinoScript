&lt;html&gt;
&lt;head&gt;
  &lt;title&gt;Generación de terreno aleatorio en 2D&lt;/title&gt;
  &lt;style&gt;
    body {
      background-color: #87CEEB;
    }
    .block {
      width: 10px;
      height: 10px;
      display: inline-block;
    }
    .grass {
      background-image: url("https://blogger.googleusercontent.com/img/b/R29vZ2xl/AVvXsEhOnABDr7hhTOsaUYC46702n22p-FY3qqr-oNUdOWwDPixoMfQ49EFwamg6wddJ8ZuCKwKLG8lGXl3ZRZdAqzrmmN_LhLENWup_ffi9hjsaVSIU3gq_mff4-HBL_ECg4LW-AAL4CsIPWGjodgWyMu1ORFRWbGb3smVX9ovM85HBYHJ2TNUrj6c_qKBPiQ/s192/2023-03-04_07.51.50.png");
      background-position: center;
      
      background-size: contain;
    }
    .dirt {
      background-image: url("https://blogger.googleusercontent.com/img/b/R29vZ2xl/AVvXsEio-0X12fmu8Tej3EZMb_oKdOOTFUhoWghkZk_NBLtmtgPhJ3sOLMml39gkhvJjE5z-UyRGT303kTrFIFiFFNn1dLdCg7K4ZUKAUk-A85kPMdDKX-lAASV8MIBr-8vA4gGT2s6E-s6qRHpyudwy-Pwv_eDO0rt1hUAo4Y1Kbv_dngnYqXXA0OgpR1UQsw/s192/2023-03-04_07.50.55.png");
      background-position: center;
      
      background-size: contain;
    }
    .stone {
      background-image: url("https://blogger.googleusercontent.com/img/b/R29vZ2xl/AVvXsEhsH0qnraVSIAnq1900opdNIuLZTN2YVM-zzpNQVtXxfy4R8BDzcHvBycvRfa0WsYokR-ASqJ4kHGmFUnwotV_iXj6gPWgq1MKdLeXuncedjjxG3ANuUOzQOFeORywGGZRQOaV60GHGHNLoZXJ_f9ZnFK8pws4W2NgHGDy3GSVFqx4DJ69lPzSV8NxBHg/s192/2023-03-06_00.02.07.png");
      background-position: center;
      
      background-size: contain;
    }
  &lt;/style&gt;
&lt;/head&gt;
&lt;body&gt;
  &lt;script&gt;
    const width = 50;
    var height = ~~(Math.random() * (50 - 47) + 47);

    const blocks = [
      'grass',
      'dirt',
      'stone'
    ];
    function generateTerrain() {
      const terrain = [];

      for (let y = 0; y &lt; height; y++) {
        height = ~~(Math.random() * (50 - 47) + 47);
        const row = [];

        for (let x = 0; x &lt; width; x++) {
          const blockIndex = Math.floor(Math.random() * blocks.length);
          const blockType = blocks[blockIndex];
          row.push(blockType);
        }

        terrain.push(row);
      }

      return terrain;
    }
    
    function renderTerrain(terrain) {
      const container = document.createElement('div');

      for (let y = 0; y &lt; height; y++) {
        
        for (let x = 0; x &lt; width; x++) {
          const blockType = terrain[y][x];
          const blockElement = document.createElement('div');
          blockElement.classList.add('block', blockType);
          container.appendChild(blockElement);
        }
        
      }

      document.body.appendChild(container);
    }

    const terrain = generateTerrain();
    renderTerrain(terrain);
  &lt;/script&gt;
&lt;/body&gt;
&lt;/html&gt;</content><link rel='edit' type='application/atom+xml' href='https://www.blogger.com/feeds/5737285046159747269/pages/default/3835645760252119147'/><link rel='self' type='application/atom+xml' href='https://www.blogger.com/feeds/5737285046159747269/pages/default/3835645760252119147'/><author><name>Trollhunters501</name><uri>https://www.blogger.com/profile/13433536723278304432</uri><email>noreply@blogger.com</email><gd:image rel='http://schemas.google.com/g/2005#thumbnail' width='32' height='32' src='//blogger.googleusercontent.com/img/b/R29vZ2xl/AVvXsEi8hesgmE-pfSldMnYbOmNMQT7PK6j7SPCjRH1AWA91j-iDiNstgk8Karm6gIsMvjokBLOOpfaioIJGaYEREDOIOC8_waak_QRpaG4XCK9TvLbfl1vBNiQPg2i9c5LyWuU/s113/20220823_182853_0000.png'/></author></entry><entry><id>tag:blogger.com,1999:blog-5737285046159747269.page-2627985133002222755</id><published>2023-06-03T00:24:00.001-07:00</published><updated>2023-06-03T10:45:00.786-07:00</updated><app:control xmlns:app='http://purl.org/atom/app#'><app:draft>yes</app:draft></app:control><category scheme='http://schemas.google.com/g/2005#kind' term='http://schemas.google.com/blogger/2008/kind#page'/><title type='text'>Test 1 de Chat Bot</title><content type='html'>&lt;!DOCTYPE html&gt;
&lt;html&gt;
  &lt;head&gt;
    &lt;meta charset="UTF-8"&gt;
    &lt;title&gt;Chatbot&lt;/title&gt;
    &lt;style&gt;
      #chat {
        border: 1px solid black;
        height: 200px;
        overflow-y: scroll;
        margin-bottom: 10px;
      }

      .user {
        background-color: #f1f1f1;
        padding: 10px;
        margin-bottom: 5px;
      }

      .bot {
        background-color: #e6e6e6;
        padding: 10px;
        margin-bottom: 5px;
      }
    &lt;/style&gt;
  &lt;/head&gt;
  &lt;body&gt;
    &lt;h1&gt;IA Maxi Chatbot&lt;/h1&gt;

    &lt;div id="chat"&gt;&lt;/div&gt;

    &lt;input type="text" id="input"&gt;
    &lt;button id="submit"&gt;Enviar&lt;/button&gt;

    &lt;script&gt;
      // Define una matriz de respuestas
var responsesHello = [
  "Hola",
  "Hola, ¿Cómo estás?",
  "Hola!, Esto sera Divertido 😎",
  "Hola!"
];
var responsesIndef = [
  "No te entiendo! Soy un Bot Muy básico y estoy aprendiendo!",
  "Podrías reformular tu pregunta? Soy un Bot Muy básico y estoy aprendiendo!",
  "Ho lo siento no te entiendo! Soy un Bot Muy básico y estoy aprendiendo!"
];
var responsesSearch = [
  "Buscando... Espera!\nListo!",
  "Buscando información..\nListo!",
  "Busco y Busco....\nListo!",
  "Buscando🕵️...\nListo!"
];
// Obtén el elemento de entrada de texto y el botón de envío del DOM
var inputField = document.getElementById("input");
var submitButton = document.getElementById("submit");

// Obtén el elemento de chat del DOM
var chat = document.getElementById("chat");

// Agrega un controlador de eventos para el botón Enviar
submitButton.addEventListener("click", function() {
  // Obtén el valor del campo de entrada de texto
  var inputValue = inputField.value.toLowerCase();
  

  // Crea un nuevo elemento de chat para la entrada del usuario
  var userChat = document.createElement("div");
  userChat.className = "user";
  userChat.innerHTML = "Usuario: "+inputValue;

  // Agrega el elemento de chat del usuario al chat
  chat.appendChild(userChat);

  // Obtén una respuesta aleatoria del conjunto de respuestas
  if(inputValue.indexOf("busca") != -1 || inputValue.indexOf("investiga") != -1){
    var searchterm = inputValue .replace("busca", "").replace("investiga", "");
    var randomIndex = Math.floor(Math.random() * responsesSearch.length);
    var responseText = responsesSearch[randomIndex];
    
    window.open("https://www.google.com/search?q=" + searchterm);
  }
  switch(inputValue.replaceAll(" ", "")){
      case "hola":
      var randomIndex = Math.floor(Math.random() * responsesHello.length);
      var responseText = responsesHello[randomIndex];
      
  }

  // Crea un nuevo elemento de chat para la respuesta del bot
  if(responseText == undefined){
    var randomIndex = Math.floor(Math.random() * responsesIndef.length);

      var responseText = responsesIndef[randomIndex];
    var botChat = document.createElement("div");
  botChat.className = "bot";
  botChat.innerHTML = "MaxiBot: "+responseText;
  }else{
  var botChat = document.createElement("div");
  botChat.className = "bot";
  botChat.innerHTML = "MaxiBot: "+responseText;
  }

  // Agrega el elemento de chat del bot al chat después de un breve retraso
  setTimeout(function() {
    chat.appendChild(botChat);
  }, Math.random() * (3000 - 500) + 500);

  // Borra el valor del campo de entrada de texto después de enviarlo
  inputField.value = "";
});

    &lt;/script&gt;
  &lt;/body&gt;
&lt;/html&gt;
</content><link rel='edit' type='application/atom+xml' href='https://www.blogger.com/feeds/5737285046159747269/pages/default/2627985133002222755'/><link rel='self' type='application/atom+xml' href='https://www.blogger.com/feeds/5737285046159747269/pages/default/2627985133002222755'/><author><name>Trollhunters501</name><uri>https://www.blogger.com/profile/13433536723278304432</uri><email>noreply@blogger.com</email><gd:image rel='http://schemas.google.com/g/2005#thumbnail' width='32' height='32' src='//blogger.googleusercontent.com/img/b/R29vZ2xl/AVvXsEi8hesgmE-pfSldMnYbOmNMQT7PK6j7SPCjRH1AWA91j-iDiNstgk8Karm6gIsMvjokBLOOpfaioIJGaYEREDOIOC8_waak_QRpaG4XCK9TvLbfl1vBNiQPg2i9c5LyWuU/s113/20220823_182853_0000.png'/></author></entry><entry><id>tag:blogger.com,1999:blog-5737285046159747269.page-7155565258137042936</id><published>2023-06-03T01:27:00.001-07:00</published><updated>2023-06-03T01:34:47.535-07:00</updated><app:control xmlns:app='http://purl.org/atom/app#'><app:draft>yes</app:draft></app:control><category scheme='http://schemas.google.com/g/2005#kind' term='http://schemas.google.com/blogger/2008/kind#page'/><title type='text'>Npc </title><content type='html'>&lt;script&gt;
class NPC {
  constructor(name, health, damage) {
    this.name = name;
    this.health = health;
    this.damage = damage;
  }

  attack(target) {
    console.log(this.name + ' ataca a '+ target.name + ' y le hace '+this.damage+' puntos de daño.');
    alert(this.name + ' ataca a '+ target.name + ' y le hace '+this.damage+' puntos de daño.');
    target.health -= this.damage;
  }
}

const player = { name: 'Jugador', health: 100, damage: 10 };
const npc = new NPC('Enemigo', 50, 50);

npc.attack(player);
