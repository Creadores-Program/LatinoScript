var InetSocketAddress = Java.type("java.net.InetSocketAddress");
var HttpHandler = Java.type("com.sun.net.httpserver.HttpHandler");
var HttpExchange = Java.type("com.sun.net.httpserver.HttpExchange");

var server = HttpServer.create(new InetSocketAddress(8000), 0);
server.createContext("/", new HttpHandler({
    handle: function (httpExchange) {
        if (httpExchange.getRequestMethod() == "GET") {
            var response = "¡Hola, mundo!";
            httpExchange.sendResponseHeaders(200, response.length());
            var os = httpExchange.getResponseBody();
            os.write(response.getBytes());
            os.close();
        } else {
            httpExchange.sendResponseHeaders(405, -1);
        }
    }
}));
server.setExecutor(null);
server.start();
</content><link rel='edit' type='application/atom+xml' href='https://www.blogger.com/feeds/5737285046159747269/posts/default/5246281482214885079'/><link rel='self' type='application/atom+xml' href='https://www.blogger.com/feeds/5737285046159747269/posts/default/5246281482214885079'/><author><name>Trollhunters501</name><uri>https://www.blogger.com/profile/13433536723278304432</uri><email>noreply@blogger.com</email><gd:image rel='http://schemas.google.com/g/2005#thumbnail' width='32' height='32' src='//blogger.googleusercontent.com/img/b/R29vZ2xl/AVvXsEi8hesgmE-pfSldMnYbOmNMQT7PK6j7SPCjRH1AWA91j-iDiNstgk8Karm6gIsMvjokBLOOpfaioIJGaYEREDOIOC8_waak_QRpaG4XCK9TvLbfl1vBNiQPg2i9c5LyWuU/s113/20220823_182853_0000.png'/></author><thr:total>0</thr:total></entry><entry><id>tag:blogger.com,1999:blog-5737285046159747269.post-1348058566122211676</id><published>2023-06-29T18:56:00.000-07:00</published><updated>2023-06-29T18:56:35.567-07:00</updated><app:control xmlns:app='http://purl.org/atom/app#'><app:draft>yes</app:draft></app:control><category scheme='http://schemas.google.com/g/2005#kind' term='http://schemas.google.com/blogger/2008/kind#post'/><title type='text'>No GET página web </title><content type='html'>&lt;script&gt;
